<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
	<Appenders>
		<File name="File" fileName=".\\report.log">
			<PatternLayout pattern="[%d{ISO8601} %-5level] %logger{36} - %msg%n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="all">
			<AppenderRef ref="File" level="all" />
		</Root>
	</Loggers>
</Configuration>